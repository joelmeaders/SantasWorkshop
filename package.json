{
	"name": "santasworkshop",
	"version": "4.3.0",
	"author": "Joel Meaders",
	"private": true,
	"workspaces": [
		"santashop-admin",
		"santashop-app",
		"santashop-core",
		"santashop-functions",
		"santashop-models"
	],
	"scripts": {
		"ng": "ng",
		"functions:deploy:test": "firebase use santas-workshop-test && firebase deploy --only functions",
		"functions:deploy:prod": "firebase use santas-workshop-193b5 && firebase deploy --only functions",
		"emulators:start": "firebase use santas-workshop-test && firebase emulators:start --import .firebase --export-on-exit",
		"emulators:start:noexport": "firebase use santas-workshop-test && firebase emulators:start --import .firebase",
		"emulators:start:sample": "firebase use santas-workshop-test && firebase emulators:start --import .firebase-sample-data --export-on-exit .firebase-sample-data",
		"fsrules:deploy": "firebase deploy --only firestore:rules",
		"prettier:format": "npx prettier --end-of-line auto --write .",
		"ci:admin:config": "ts-node -O '{\"module\": \"commonjs\"}' config.admin.ts",
		"ci:admin:build": "npm run build -w @santashop/models && npm run build:prod -w @santashop/core && ng build santashop-admin --configuration=production",
		"ci:app:config": "ts-node -O '{\"module\": \"commonjs\"}' config.app.ts",
		"ci:app:build": "npm run build -w @santashop/models && npm run build:prod -w @santashop/core && ng build santashop-app --configuration=production"
	},
	"dependencies": {
		"@angular/animations": "^18.2.6",
		"@angular/common": "^18.2.6",
		"@angular/compiler": "^18.2.6",
		"@angular/core": "^18.2.6",
		"@angular/fire": "^18.0.1",
		"@angular/forms": "^18.2.6",
		"@angular/platform-browser": "^18.2.6",
		"@angular/platform-browser-dynamic": "^18.2.6",
		"@angular/router": "^18.2.6",
		"@ionic/angular": "^8.3.1",
		"@ngx-translate/core": "^15.0.0",
		"@ngx-translate/http-loader": "^8.0.0",
		"@zxing/browser": "^0.1.5",
		"@zxing/library": "^0.21.3",
		"@zxing/ngx-scanner": "^18.0.1",
		"chart.js": "^4.4.4",
		"chartjs-plugin-datalabels": "^2.2.0",
		"date-fns": "^4.1.0",
		"firebase": "^10.13.2",
		"ionicons": "^7.4.0",
		"ng2-charts": "^6.0.1",
		"rxjs": "^7.8.1",
		"tslib": "~2.7.0",
		"zone.js": "~0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.2.6",
		"@angular-eslint/builder": "18.3.1",
		"@angular-eslint/eslint-plugin": "18.3.1",
		"@angular-eslint/eslint-plugin-template": "18.3.1",
		"@angular-eslint/schematics": "18.3.1",
		"@angular-eslint/template-parser": "18.3.1",
		"@angular/cli": "^18.2.11",
		"@angular/compiler-cli": "^18.2.6",
		"@hirez_io/observer-spy": "^2.2.0",
		"@ionic/angular-toolkit": "^11.0.1",
		"@ionic/cli": "^7.2.0",
		"@types/jasmine": "^5.1.4",
		"@types/node": "^22.7.4",
		"@types/underscore": "^1.11.15",
		"@typescript-eslint/eslint-plugin": "^8.7.0",
		"@typescript-eslint/parser": "8.7.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-jsdoc": "^50.3.0",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"eslint-plugin-prettier": "^5.2.1",
		"firebase-tools": "^13.20.1",
		"jasmine-core": "~5.3.0",
		"jasmine-spec-reporter": "~7.0.0",
		"karma": "~6.4.4",
		"karma-chrome-launcher": "~3.2.0",
		"karma-cli": "^2.0.0",
		"karma-coverage": "~2.2.1",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "^2.1.0",
		"ng-packagr": "^18.2.1",
		"open": "^10.1.0",
		"prettier": "^3.3.3",
		"ts-node": "~10.9.2",
		"typescript": "5.4.5",
		"webpack-bundle-analyzer": "^4.10.2"
	},
	"volta": {
		"node": "22.9.0"
	}
}
