{
  "name": "santasworkshop",
  "version": "3.0.0-preview1",
  "author": "Joel Meaders",
  "private": true,
  "scripts": {
    "ng": "ng",
    "core:lint": "ng lint santashop-core --fix --cache && prettier --end-of-line auto --write santashop-core",
    "models:lint": "ng lint santashop-models --fix --cache && prettier --end-of-line auto --write santashop-models",
    "app:lint": "ng lint santashop-app --fix --cache && prettier --end-of-line auto --write santashop-app",
    "core:test": "ng test santashop-core",
    "core:test:coverage": "ng test santashop-core --no-watch --code-coverage",
    "core:build": "ng build santashop-core",
    "models:build": "ng build santashop-models",
    "admin:lint": "ng lint santashop-admin --fix --cache",
    "admin:test": "ng test santashop-admin",
    "admin:test:coverage": "ng test santashop-admin --no-watch --code-coverage",
    "admin:start": "firebase use santas-workshop-193b5 && ng serve santashop-admin --port=4100",
    "admin:start:qa": "firebase use santas-workshop-test && ng serve santashop-admin --port=4100",
    "admin:config": "ts-node -O {\\\"module\\\":\\\"commonjs\\\"} ./config.admin.ts",
    "admin:build:local": "ng build santashop-admin --configuration=production",
    "admin:build:qa": "ng build santashop-admin --configuration=qa",
    "admin:analyze": "ng build santashop-admin --configuration=production --named-chunks=true --stats-json && webpack-bundle-analyzer dist/santashop-admin/stats.json",
    "app:lint:fix": "ng lint santashop-app --fix --cache",
    "app:test": "ng test santashop-app",
    "app:test:coverage": "ng test santashop-app --no-watch --code-coverage",
    "app:start": "firebase use qa && ng serve santashop-app --port=4100",
    "app:start:qa": "firebase use santas-workshop-test && ng serve santashop-app --port=4100",
    "app:config": "ts-node -O {\\\"module\\\":\\\"commonjs\\\"} ./config.client.ts",
    "app:build:local": "ng build santashop-app --configuration=production",
    "app:build:qa": "ng build santashop-app --configuration=qa",
    "app:analyze": "ng build santashop-app --configuration=production --named-chunks=true --stats-json && webpack-bundle-analyzer dist/santashop-app/stats.json",
    "app:deploy:qa": "firebase use santas-workshop-test && firebase target santashop-app && ng build santashop-app --configuration=qa && firebase deploy",
    "app:deploy:qa:hosting": "firebase use santas-workshop-test && firebase target santashop-app && ng build santashop-app --configuration=qa && firebase deploy --only hosting",
    "app:deploy:qa:functions": "firebase use santas-workshop-test && firebase deploy --only functions",
    "ci:admin:config": "ts-node -O '{\"module\": \"commonjs\"}' config.admin.ts",
    "ci:admin:build": "npm run models:build && npm run core:build && ng build santashop-admin --configuration=production",
    "ci:app:config": "ts-node -O '{\"module\": \"commonjs\"}' config.app.ts",
    "ci:app:build": "npm run models:build && npm run core:build && ng build santashop-app --configuration=production",
    "emulators:start": "firebase use qa && firebase emulators:start --import .firebase --export-on-exit",
    "emulators:start:noexport": "firebase use qa && firebase emulators:start --import .firebase",
    "fsrules:deploy": "firebase deploy --only firestore:rules",
    "prettier:format": "npx prettier --end-of-line auto --write ."
  },
  "dependencies": {
    "@angular/animations": "^14.2.8",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/router": "^14.2.8",
    "@ionic/angular": "^6.3.4",
    "@ngneat/reactive-forms": "^5.0.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@zxing/browser": "0.1.1",
    "@zxing/library": "^0.19.1",
    "@zxing/ngx-scanner": "^3.6.2",
    "date-fns": "^2.29.3",
    "ng-recaptcha": "^10.0.0",
    "rxjs": "^7.5.7",
    "tslib": "~2.4.1",
    "underscore": "^1.13.6",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.8",
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular-devkit/core": "^14.2.8",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^14.2.8",
    "@angular/compiler-cli": "^14.2.8",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@firebase/rules-unit-testing": "^2.0.5",
    "@ionic/angular-toolkit": "^7.0.0",
    "@ngneat/eslint-plugin-reactive-forms": "^4.0.0",
    "@types/grecaptcha": "^3.0.4",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.11.9",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.6.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-tools": "^11.16.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-cli": "^2.0.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^14.2.2",
    "open": "^8.4.0",
    "prettier": "^2.7.1",
    "ts-node": "~10.9.1",
    "typescript": "4.8.4",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}