    "inquirer-autocomplete-prompt": "^1.4.0",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.1.2",
    "open": "^8.4.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "4.5.4"
  }
}
