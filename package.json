{
  "name": "sw",
  "version": "2.0.8",
  "author": "Joel Meaders",
  "private": true,
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "e2e": "ng e2e",
    "core:lint": "ng lint --fix --project=santashop-core",
    "core:build": "ng build --project=santashop-core",
    "models:build": "ng build --project=santashop-models",
    "admin:lint": "npm run core:lint && ng lint --fix --cache --project=santashop-admin",
    "admin:start": "firebase use santas-workshop-193b5 && ng serve --project=santashop-admin --port=4100",
    "admin:config": "ts-node -O {\\\"module\\\":\\\"commonjs\\\"} ./config.admin.ts",
    "admin:build:local": "ng build --configuration=production --project=santashop-admin",
    "admin:analyze": "ng build --configuration production --named-chunks=true --project=santashop-admin --stats-json && webpack-bundle-analyzer dist/santashop-admin/stats.json",
    "app:lint": "npm run core:lint && ng lint --fix --project=santashop-app",
    "app:start": "firebase use santas-workshop-193b5 && ng serve --project=santashop-app --port=4100",
    "app:start:qa": "firebase use santas-workshop-test && ng serve --project=santashop-app --port=4100",
    "app:config": "ts-node -O {\\\"module\\\":\\\"commonjs\\\"} ./config.client.ts",
    "app:build:local": "ng build --configuration=production --project=santashop-app",
    "app:build:qa": "ng build --configuration=qa --project=santashop-app",
    "app:analyze": "ng build --configuration=production --named-chunks=true --project=santashop-app --stats-json && webpack-bundle-analyzer dist/santashop-app/stats.json",
    "app:deploy:qa": "firebase use santas-workshop-test && firebase target santashop-app && ng build --configuration=qa --project=santashop-app && firebase deploy",
    "app:deploy:qa:hosting": "firebase use santas-workshop-test && firebase target santashop-app && ng build --configuration=qa --project=santashop-app && firebase deploy --only hosting",
    "app:deploy:qa:functions": "firebase use santas-workshop-test && firebase deploy --only functions",
    "ci:admin:config": "ts-node -O '{\"module\": \"commonjs\"}' config.admin.ts",
    "ci:admin:build": "npm run models:build && npm run core:build && ng build --configuration production --project=santashop-admin",
    "ci:app:config": "ts-node -O '{\"module\": \"commonjs\"}' config.app.ts",
    "ci:app:build": "npm run models:build && npm run core:build && ng build --configuration production --project=santashop-app",
    "emulators:start": "firebase emulators:start --import .firebase --export-on-exit",
    "emulators:start:noexport": "firebase emulators:start --import .firebase",
    "fsrules:deploy": "firebase deploy --only firestore:rules"
  },
  "dependencies": {
    "@angular/animations": "^14.0.3",
    "@angular/common": "^14.0.3",
