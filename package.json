    "@zxing/browser": "0.1.1",
    "@zxing/library": "^0.19.1",
    "@zxing/ngx-scanner": "^3.6.2",
    "date-fns": "^2.28.0",
    "firebase": "^9.8.4",
    "ng-recaptcha": "^10.0.0",
    "rxfire": "^6.0.3",
    "rxjs": "^7.5.5",
    "tslib": "~2.4.0",
    "underscore": "^1.13.4",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1400.3",
