{
	"name": "santasworkshop",
	"version": "3.6.6",
	"author": "Joel Meaders",
	"private": true,
	"workspaces": [
		"santashop-admin",
		"santashop-app",
		"santashop-core",
		"santashop-functions",
		"santashop-models"
	],
	"scripts": {
		"ng": "ng",
		"functions:deploy:test": "firebase use santas-workshop-test && firebase deploy --only functions",
		"functions:deploy:prod": "firebase use santas-workshop-193b5 && firebase deploy --only functions",
		"emulators:start": "firebase use santas-workshop-test && firebase emulators:start --import .firebase --export-on-exit",
		"emulators:start:noexport": "firebase use santas-workshop-test && firebase emulators:start --import .firebase",
		"emulators:start:sample": "firebase use santas-workshop-test && firebase emulators:start --import .firebase-sample-data --export-on-exit .firebase-sample-data",
		"fsrules:deploy": "firebase deploy --only firestore:rules",
		"prettier:format": "npx prettier --end-of-line auto --write .",
		"ci:admin:config": "ts-node -O '{\"module\": \"commonjs\"}' config.admin.ts",
		"ci:admin:build": "npm run build -w @santashop/models && npm run build:prod -w @santashop/core && ng build santashop-admin --configuration=production",
		"ci:app:config": "ts-node -O '{\"module\": \"commonjs\"}' config.app.ts",
		"ci:app:build": "npm run build -w @santashop/models && npm run build:prod -w @santashop/core && ng build santashop-app --configuration=production"
	},
	"dependencies": {
		"@angular/animations": "^16.2.10",
		"@angular/common": "^16.2.10",
		"@angular/compiler": "^16.2.10",
		"@angular/core": "^16.2.10",
		"@angular/fire": "^7.6.1",
		"@angular/forms": "^16.2.10",
		"@angular/platform-browser": "^16.2.10",
		"@angular/platform-browser-dynamic": "^16.2.10",
		"@angular/router": "^16.2.10",
		"@ionic/angular": "^7.5.1",
		"@ngneat/reactive-forms": "^5.0.2",
		"@ngx-translate/core": "^15.0.0",
		"@ngx-translate/http-loader": "^8.0.0",
		"@zxing/browser": "0.1.4",
		"@zxing/library": "^0.20.0",
		"@zxing/ngx-scanner": "^16.1.0",
		"chart.js": "^4.4.0",
		"chartjs-plugin-datalabels": "^2.2.0",
		"date-fns": "^2.30.0",
		"firebase": "9.8.2",
		"ng2-charts": "^5.0.3",
		"rxfire": "6.0.3",
		"rxjs": "^7.8.1",
		"tslib": "~2.6.2",
		"zone.js": "~0.13.0"
	},
	"devDependencies": {
		"@angular-devkit/architect": "^0.1602.6",
		"@angular-devkit/build-angular": "^16.2.6",
		"@angular-devkit/core": "^16.2.6",
		"@angular-eslint/builder": "16.2.0",
		"@angular-eslint/eslint-plugin": "16.2.0",
		"@angular-eslint/eslint-plugin-template": "16.2.0",
		"@angular-eslint/schematics": "16.2.0",
		"@angular-eslint/template-parser": "16.2.0",
		"@angular/cdk": "^16.2.9",
		"@angular/cli": "^16.2.6",
		"@angular/compiler-cli": "^16.2.10",
		"@biesbjerg/ngx-translate-extract": "^7.0.4",
		"@hirez_io/observer-spy": "^2.2.0",
		"@ionic/angular-toolkit": "^10.0.0",
		"@ngneat/eslint-plugin-reactive-forms": "^4.0.0",
		"@types/grecaptcha": "^3.0.6",
		"@types/jasmine": "^5.1.1",
		"@types/node": "^20.8.7",
		"@types/underscore": "^1.11.12",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "6.8.0",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsdoc": "^46.8.2",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"eslint-plugin-prettier": "^5.0.1",
		"firebase-tools": "^12.7.0",
		"jasmine-core": "~5.1.1",
		"jasmine-spec-reporter": "~7.0.0",
		"karma": "~6.4.2",
		"karma-chrome-launcher": "~3.2.0",
		"karma-cli": "^2.0.0",
		"karma-coverage": "~2.2.1",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "^2.1.0",
		"ng-packagr": "^16.2.3",
		"open": "^9.1.0",
		"prettier": "^3.0.3",
		"ts-node": "~10.9.1",
		"typescript": "4.9.3",
		"webpack-bundle-analyzer": "^4.9.1"
	}
}
